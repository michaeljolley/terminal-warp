{
  "name": "terminal-warp",
  "version": "0.0.95",
  "description": "NPM command that installs Warp Terminal actions into Windows Terminal.",
  "main": "bin/index.js",
  "bin": {
    "terminal-warp": "bin/index.js"
  },
  "scripts": {
    "start": "node ./bin/index.js",
    "build": "esbuild src/index.ts --platform=neutral --packages=external --bundle --outfile=bin/index.js",
    "format": "prettier --write .",
    "lint": "eslint -c eslint.config.mjs ."
  },
  "type": "module",
  "author": "Michael Jolley <mike@baldbeardedbuilder.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaeljolley/terminal-warp.git"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/titlecase": "^1.1.2",
    "@types/tmp": "^0.2.6",
    "@types/unzipper": "^0.10.10",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "titlecase": "^1.1.3",
    "tmp": "^0.2.3",
    "unzipper": "^0.12.3",
    "yaml": "^2.5.1"
  }
}
